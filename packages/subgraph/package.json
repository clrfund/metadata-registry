    "mustache": "^4.2.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.26.0",
    "@graphprotocol/graph-ts": "^0.24.1",
    "as-string-sink": "^0.5.0"
